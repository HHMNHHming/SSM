<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace 与哪个Mapper对应-->
<mapper namespace="com.gwm.dao.IdCardDao">
    <select id="selectCodeById" parameterType="Integer" resultType="com.gwm.po.Idcard">
        select id,code from idcard where id = #{id}
    </select>
</mapper>